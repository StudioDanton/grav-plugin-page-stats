<table class="table table-hover">
    <tr>
        <th>
            {{ "PLUGIN_PAGE_STATS.TABLE_BROWSER"|t }}
        </th>
        <th>
            {{ "PLUGIN_PAGE_STATS.TABLE_SHARE"|t }}
        </th>
    </tr>
    {% for s in data %}
        <tr>
            <td>
                {{ s.browser }}
            </td>
            <td>
                <span title="{{ s.hits }} {{ "PLUGIN_PAGE_STATS.TABLE_VIEWS"|t }}">{{ s.share }}
                    %</span>
            </td>
        </tr>
    {% endfor %}
</table>
